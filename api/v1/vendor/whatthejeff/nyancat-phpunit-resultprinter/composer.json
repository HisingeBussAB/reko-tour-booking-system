    "keywords": ["nyan","cat","rainbow","phpunit","tests"],
    "homepage": "http://github.com/whatthejeff/nyancat-phpunit-resultprinter",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Welch",
            "email": "whatthejeff@gmail.com",
            "homepage": "http://whatthejeff.com"
        }
    ],
    "support": {
        "issues": "https://github.com/whatthejeff/nyancat-phpunit-resultprinter/issues"
    },
    "require": {
