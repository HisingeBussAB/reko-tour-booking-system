  "dependencies": {
    "@firebase/app-types": "0.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-regular-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.1",
    "bootstrap": "^4.0.0",
    "connected-react-router": "^4.3.0",
    "firebase": "^4.11.0",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.6",
    "lodash": "^4.17.15",
    "moment": "^2.21.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-bootstrap-typeahead": "^4.0.0-alpha.10",
    "react-datepicker": "^1.2.2",
    "react-dom": "^16.2.0",
    "react-dropdown": "^1.6.4",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.1",
    "react-textfit": "^1.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sha.js": "^2.4.11",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/styles/scss/ -o ./src/styles/css/ --output-style expanded --source-map true",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles/scss/ -o ./src/styles/css/ --watch --recursive --output-style expanded --source-map true",
    "build-css-prod": "node-sass-chokidar ./src/styles/scss/ -o ./src/styles/css/ --output-style compressed -x",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css-prod build-js && sw-precache --config=sw-precache-config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.0",
    "eslint": "^4.18.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "redux-freeze": "^0.1.5",
    "sw-precache": "^5.2.1"
  }
}
